#compdef monero-gen-trusted-multisig

_monero-gen-trusted-multisig() {
  _arguments \
    '(- *)--help[Produce help message]' \
    '(- *)--version[Output version information]' \
    '--config-file=[Config file]:config file:_files' \
    '--create-address-file[Create an address file for new wallets]' \
    '--filename-base=[Base filename \(-1, -2, etc suffixes will be appended as needed\)]:filename base:_files' \
    '--log-file=[Specify log file]:log file:_files' \
    '--log-level=[0-4 or categories]:log level' \
    '--max-concurrency=[Max number of threads to use for a parallel job]:threads' \
    '--max-log-file-size=[Specify maximum log file size \[B\]]:size (bytes)' \
    '--max-log-files=[Specify maximum number of rotated log files to be saved \(no limit by setting to 0\)]:count' \
    '--participants=[How many participants will share parts of the multisig wallet]:participants' \
    '--scheme=[Give threshold and participants at once as M/N]:scheme (M/N)' \
    '(--testnet)--stagenet[Create stagenet multisig wallets]' \
    '(--stagenet)--testnet[Create testnet multisig wallets]' \
    '--threshold=[How many signers are required to sign a valid transaction]:threshold'
}

_monero-gen-trusted-multisig "$@"
