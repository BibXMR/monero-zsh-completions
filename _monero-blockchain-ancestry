#compdef monero-blockchain-ancestry

_monero-blockchain-ancestry() {
  _arguments \
    '(- *)--help[Produce help message]' \
    '--cache-blocks[Cache blocks \(memory hungry\)]' \
    '--cache-outputs[Cache outputs \(memory hungry\)]' \
    '--cache-txes[Cache txes \(memory hungry\)]' \
    '--data-dir=[Specify data directory]:data directory:_directories' \
    '--height=[Get ancestry for all txes at this height]:block height' \
    '--include-coinbase[Including coinbase tx in per height average]' \
    '--log-level=[0-4 or categories]:log level' \
    '--output=[Get ancestry for this output \(amount/offset format\)]:amount/offset' \
    '--refresh[Refresh the whole chain first]' \
    '--show-cache-stats[Show cache statistics]' \
    '(--testnet)--stagenet[Run on stagenet.]' \
    '(--stagenet)--testnet[Run on testnet.]' \
    '--txid=[Get ancestry for this txid]:transaction hash'
}

_monero-blockchain-ancestry "$@"
