#compdef monero-wallet-rpc

_monero-wallet-rpc() {
  _arguments \
    '(- *)--help[Produce help message]' \
    '(- *)--version[Output version information]' \
    '--allow-mismatched-daemon-version[Allow communicating with a daemon that uses a different version]' \
    '--bitmessage-address=[Use PyBitmessage instance at URL <arg>]:URL' \
    '--bitmessage-login=[Specify <arg> as username\:password for PyBitmessage API]:credentials (user\:pass)' \
    '--config-file=[Config file]:config file:_files' \
    '--confirm-external-bind[Confirm rpc-bind-ip value is NOT a loopback \(local\) IP]' \
    '--daemon-address=[Use daemon instance at <host>\:<port>]:daemon address (host\:port)' \
    '--daemon-host=[Use daemon instance at host <arg> instead of localhost]:hostname' \
    '--daemon-login=[Specify username\[\:password\] for daemon RPC client]:credentials (user\:pass)' \
    '--daemon-port=[Use daemon instance at port <arg> instead of 18081]:port' \
    '--daemon-ssl=[Enable SSL on daemon RPC connections\: enabled|disabled|autodetect]:daemon-ssl:(enabled disabled autodetect)' \
    '--daemon-ssl-allow-any-cert[Allow any SSL certificate from the daemon]' \
    '--daemon-ssl-allow-chained[Allow user \(via --daemon-ssl-ca-certificates\) chain certificates]' \
    '*--daemon-ssl-allowed-fingerprints=[List of valid fingerprints of allowed RPC servers]:fingerprint' \
    '--daemon-ssl-ca-certificates=[Path to file containing concatenated PEM format certificate\(s\) to replace system CA\(s\).]:daemon-ssl-ca-certificates:_files' \
    '--daemon-ssl-certificate=[Path to a PEM format certificate]:daemon-ssl-certificate:_files' \
    '--daemon-ssl-private-key=[Path to a PEM format private key]:daemon-ssl-private-key:_files' \
    '--disable-rpc-ban[Do not ban hosts on RPC errors]' \
    '--disable-rpc-login[Disable HTTP authentication for RPC connections served by this process]' \
    '--extra-entropy=[File containing extra entropy to initialize the PRNG \(any data, aim for 256 bits of entropy to be useful, which typically means more than 256 bits of data\)]:entropy file:_files' \
    '--generate-from-json=[Generate wallet from JSON format file]:JSON file:_files' \
    '--hw-device=[HW device to use]:device' \
    '--hw-device-deriv-path=[HW device wallet derivation path \(e.g., SLIP-10\)]:derivation path' \
    '--kdf-rounds=[Number of rounds for the key derivation function]:rounds' \
    '--log-file=[Specify log file]:log file:_files' \
    '--log-level=[0-4 or categories]:log level' \
    '--max-concurrency=[Max number of threads to use for a parallel job]:threads' \
    '--max-log-file-size=[Specify maximum log file size \[B\]]:size (bytes)' \
    '--max-log-files=[Specify maximum number of rotated log files to be saved \(no limit by setting to 0\)]:count' \
    '--no-dns[Do not use DNS]' \
    '--no-initial-sync[Skips the initial sync before listening for connections]' \
    '--non-interactive[Run non-interactive]' \
    '--offline[Do not connect to a daemon, nor use DNS]' \
    '--password=[Wallet password \(escape/quote as needed\)]:password' \
    '--password-file=[Wallet password file]:password file:_files' \
    '--prompt-for-password[Prompts for password when not provided]' \
    '--proxy=[\[<ip>\:\]<port> socks proxy to use for daemon connections]:proxy (ip\:port)' \
    '--restricted-rpc[Restricts to view-only commands]' \
    '--rpc-access-control-origins=[Specify a comma separated list of origins to allow cross origin resource sharing]:origins' \
    '--rpc-bind-ip=[Specify IP to bind RPC server]:IP address' \
    '--rpc-bind-ipv6-address=[Specify IPv6 address to bind RPC server]:IPv6 address' \
    '--rpc-bind-port=[Sets bind port for server]:port' \
    '--rpc-ignore-ipv4[Ignore unsuccessful IPv4 bind for RPC]' \
    '--rpc-login=[Specify username\[\:password\] required for RPC server]:credentials (user\:pass)' \
    '--rpc-max-connections=[Max RPC connections permitted]:count' \
    '--rpc-max-connections-per-private-ip=[Max RPC connections per private and localhost IP permitted]:count' \
    '--rpc-max-connections-per-public-ip=[Max RPC connections per public IP permitted]:count' \
    '--rpc-response-soft-limit=[Max response bytes that can be queued, enforced at next response attempt]:bytes' \
    '--rpc-restricted-bind-ip=[Specify IP to bind restricted RPC server]:IP address' \
    '--rpc-restricted-bind-ipv6-address=[Specify IPv6 address to bind restricted RPC server]:IPv6 address' \
    '--rpc-ssl=[Enable SSL on RPC connections\: enabled|disabled|autodetect]:rpc-ssl:(enabled disabled autodetect)' \
    '--rpc-ssl-allow-any-cert[Allow any peer certificate]' \
    '--rpc-ssl-allow-chained[Allow user \(via --rpc-ssl-certificates\) chain certificates]' \
    '*--rpc-ssl-allowed-fingerprints=[List of certificate fingerprints to allow]:fingerprint' \
    '--rpc-ssl-ca-certificates=[Path to file containing concatenated PEM format certificate\(s\) to replace system CA\(s\).]:rpc-ssl-ca-certificates:_files' \
    '--rpc-ssl-certificate=[Path to a PEM format certificate]:rpc-ssl-certificate:_files' \
    '--rpc-ssl-private-key=[Path to a PEM format private key]:rpc-ssl-private-key:_files' \
    '--rpc-use-ipv6[Allow IPv6 for RPC]' \
    '--shared-ringdb-dir=[Set shared ring database path]:directory:_directories' \
    '(--testnet)--stagenet[For stagenet.]' \
    '(--stagenet)--testnet[For testnet.]' \
    '--trusted-daemon[Enable commands which rely on a trusted daemon]' \
    '--tx-notify=[Run a program for each new incoming transaction, '\''%s'\'' will be replaced by the transaction hash]:command' \
    '--untrusted-daemon[Disable commands which rely on a trusted daemon]' \
    '--wallet-dir=[Directory for newly created wallets]:wallet directory:_directories' \
    '--wallet-file=[Use wallet <arg>]:wallet file:_files'
}

_monero-wallet-rpc "$@"
